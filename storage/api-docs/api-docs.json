{
    "openapi": "3.0.0",
    "info": {
        "title": "Dating Project",
        "description": "This is a sample Dating API server.",
        "version": "1.0.0"
    },
    "paths": {
        "/api/auth/signup": {
            "post": {
                "tags": [
                    "Auth"
                ],
                "operationId": "App\\Http\\Controllers\\Api\\AuthController::signup",
                "requestBody": {
                    "description": "Create user object",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "firstname": {
                                        "type": "string"
                                    },
                                    "middlename": {
                                        "type": "string"
                                    },
                                    "lastname": {
                                        "type": "string"
                                    },
                                    "email": {
                                        "type": "string",
                                        "format": "email"
                                    },
                                    "password": {
                                        "type": "string"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "User registered"
                    }
                }
            }
        },
        "/api/auth/login": {
            "post": {
                "tags": [
                    "Auth"
                ],
                "operationId": "App\\Http\\Controllers\\Api\\AuthController::login",
                "requestBody": {
                    "description": "Returns API token with given user email and password",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "email": {
                                        "type": "string",
                                        "format": "email"
                                    },
                                    "password": {
                                        "type": "string"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "User sign in",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "access_token": {
                                            "type": "string"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/auth/logout": {
            "get": {
                "tags": [
                    "Auth"
                ],
                "operationId": "App\\Http\\Controllers\\Api\\AuthController::logout",
                "requestBody": {
                    "required": true
                },
                "responses": {
                    "200": {
                        "description": "User logout"
                    }
                }
            }
        },
        "/api/information": {
            "get": {
                "tags": [
                    "Dictionaries"
                ],
                "operationId": "App\\Http\\Controllers\\Api\\DictionaryController::information",
                "requestBody": {
                    "required": true
                },
                "responses": {
                    "200": {
                        "description": "Personal info"
                    }
                }
            }
        },
        "/api/reasons": {
            "get": {
                "tags": [
                    "Dictionaries"
                ],
                "operationId": "App\\Http\\Controllers\\Api\\DictionaryController::reasons",
                "requestBody": {
                    "required": true
                },
                "responses": {
                    "200": {
                        "description": "Why user registered"
                    }
                }
            }
        },
        "/api/user": {
            "get": {
                "tags": [
                    "User"
                ],
                "operationId": "App\\Http\\Controllers\\Api\\UserController::user",
                "requestBody": {
                    "required": true
                },
                "responses": {
                    "200": {
                        "description": "User data"
                    },
                    "403": {
                        "description": "Unauthorized"
                    },
                    "500": {
                        "description": "Server error"
                    }
                }
            },
            "put": {
                "tags": [
                    "User"
                ],
                "operationId": "App\\Http\\Controllers\\Api\\UserController::update",
                "requestBody": {
                    "required": true
                },
                "responses": {
                    "200": {
                        "description": "User data updated"
                    }
                }
            }
        },
        "/api/user/information": {
            "put": {
                "tags": [
                    "User"
                ],
                "operationId": "App\\Http\\Controllers\\Api\\UserController::updateInformation",
                "requestBody": {
                    "required": true
                },
                "responses": {
                    "200": {
                        "description": "User information updated"
                    }
                }
            }
        },
        "/api/user/filters": {
            "put": {
                "tags": [
                    "User"
                ],
                "operationId": "App\\Http\\Controllers\\Api\\UserController::updateFilters",
                "requestBody": {
                    "required": true
                },
                "responses": {
                    "200": {
                        "description": "User search filters updated"
                    }
                }
            }
        }
    }
}